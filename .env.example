# SceneGen Environment Variables
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git - it contains sensitive tokens

# Hugging Face Token (required for gated models like SAM 3D Objects)
# Get your token from: https://huggingface.co/settings/tokens
HF_TOKEN=your_huggingface_token_here
HUGGING_FACE_HUB_TOKEN=your_huggingface_token_here

# Git Personal Access Token (optional, for private repos or rate limiting)
# Get your token from: https://github.com/settings/tokens
# Only needed if you hit GitHub API rate limits or need to access private repos
GITHUB_TOKEN=

# GitLab Token (optional, if using GitLab repos)
GITLAB_TOKEN=

# CUDA Configuration (optional, usually auto-detected)
CUDA_HOME=/usr/local/cuda
CUDA_VISIBLE_DEVICES=0

# Prefect Configuration (optional)
PREFECT_API_URL=http://localhost:4200/api
PREFECT_API_KEY=

# Database Configuration (optional, if using external database)
DATABASE_URL=sqlite:///data/scenegen.db

# Logging Level (optional)
LOG_LEVEL=INFO
